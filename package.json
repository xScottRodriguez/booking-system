{
  "name": "nest-prject-skeleton",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" ",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"  --fix",
    "test": "cross-env NODE_ENV=test jest --silent --color",
    "test:verbose": " cross-env NODE_ENV=test jest --verbose",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "test:cov": "cross-env NODE_ENV=test jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "cross-env NODE_ENV=test jest --config ./test/jest-e2e.json",
    "prepare": "husky install",
    "typeorm": "yarn build && npx typeorm  -d dist/db/data-source.js",
    "migration:generate": "yarn typeorm -- migration:generate",
    "migration:run": "yarn typeorm -- migration:run",
    "migration:revert": "yarn typeorm -- migration:revert"
  },
  "dependencies": {
    "@babel/code-frame": "7.18.6",
    "@nestjs-modules/mailer": "1.8.1",
    "@nestjs/axios": "^2.0.0",
    "@nestjs/common": "9.0.0",
    "@nestjs/config": "2.2.0",
    "@nestjs/core": "9.0.0",
    "@nestjs/jwt": "10.0.1",
    "@nestjs/passport": "9.0.0",
    "@nestjs/platform-express": "9.0.0",
    "@nestjs/schedule": "^2.2.0",
    "@nestjs/serve-static": "^3.0.1",
    "@nestjs/swagger": "6.1.3",
    "@nestjs/typeorm": "9.0.1",
    "axios": "^1.3.4",
    "bcrypt": "5.1.0",
    "buffer-to-stream": "1.0.0",
    "cache-manager": "5.1.4",
    "cache-manager-redis-store": "2.0.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "cloudinary": "^1.35.0",
    "compression": "1.7.4",
    "cross-env": "7.0.3",
    "dotenv": "16.0.3",
    "firebase-admin": "^11.5.0",
    "hbs": "^4.2.0",
    "helmet": "6.0.1",
    "nodemailer": "6.8.0",
    "passport": "0.6.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "4.0.1",
    "passport-local": "1.0.0",
    "pg": "8.8.0",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "7.2.0",
    "typeorm": "0.3.11",
    "uuid": "9.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "17.3.0",
    "@commitlint/config-conventional": "17.3.0",
    "@nestjs/cli": "9.0.0",
    "@nestjs/schematics": "9.0.0",
    "@nestjs/testing": "9.0.0",
    "@types/bcrypt": "5.0.0",
    "@types/cache-manager": "4.0.2",
    "@types/cron": "^2.0.0",
    "@types/dotenv": "8.2.0",
    "@types/express": "4.17.13",
    "@types/jest": "28.1.8",
    "@types/multer": "^1.4.7",
    "@types/node": "16.0.0",
    "@types/nodemailer": "6.4.7",
    "@types/passport-google-oauth20": "^2.0.11",
    "@types/passport-jwt": "3.0.8",
    "@types/passport-local": "1.0.34",
    "@types/supertest": "2.0.11",
    "@typescript-eslint/eslint-plugin": "5.0.0",
    "@typescript-eslint/parser": "5.0.0",
    "eslint": "8.0.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import-helpers": "1.3.1",
    "eslint-plugin-prettier": "^4.2.1",
    "glob": "8.0.3",
    "husky": "8.0.2",
    "jest": "28.1.3",
    "lint-staged": "13.0.4",
    "prettier": "2.3.2",
    "source-map-support": "0.5.20",
    "sqlite3": "5.1.4",
    "supertest": "6.1.3",
    "ts-jest": "28.0.8",
    "ts-loader": "9.2.3",
    "ts-node": "10.0.0",
    "tsconfig-paths": "4.1.0",
    "typescript": "4.7.4"
  },
  "engines": {
    "node": "16.x"
  },
  "jest": {
    "verbose": true,
    "moduleDirectories": [
      "node_modules",
      "<rootDir>"
    ],
    "moduleFileExtensions": [
      "js",
      "ts",
      "json"
    ],
    "rootDir": "./",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      ".+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/../src/$1",
      "^@modules/(.*)$": "<rootDir>/../src/modules/$1"
    }
  }
}
